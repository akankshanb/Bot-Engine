{"intents": [
        {"tag": "@plotbot",
         "patterns": ["Hi", "How are you", "Is anyone there?", "Hello", "Good day", "Whats up"],
         "responses": ["Hello!", "Good to see you again!", "Hi there, how can I help?"],
         "context_set": ""
        },
        {"tag": "goodbye",
         "patterns": ["cya", "See you later", "Goodbye", "I am Leaving", "Have a Good day"],
         "responses": ["Talk to you later", "Goodbye!"],
         "context_set": ""
        },
        {"tag": "plot",
         "patterns": ["I want to plot a graph", "Need to plot something", "Draw graph", "can you plot something","plot for me"],
         "responses": ["To plot a graph, give a plot type followed by the X and Y columns you want to plot and upload the file containing data"],
         "context_set": ""
        },
        {"tag": "sample",
         "patterns": ["I need a code snippet", "give me a sample plot for", "How do I draw", "plot something for me"],
         "responses": ["Please provide a graph type", "Please provide a plot type", "What kind of plot do you want","Choose between scatterplot, boxplot and barplot"],
         "context_set": ""
        },
        {"tag": "retrieve",
         "patterns": ["need my previous plots", "need plots from", "need plots till", "all"],
         "responses": ["Write retrieve followed by either all or the dates in MM/DD/YY format"],
         "context_set": ""
        }
    ]
}